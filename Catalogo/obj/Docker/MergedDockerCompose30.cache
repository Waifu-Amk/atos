name: dockercompose12082744716569367158
services:
  catalogo.webapi:
    build:
      context: C:\Users\kevin\source\repos\atos\Catalogo
      dockerfile: Catalogo/Dockerfile
    depends_on:
      db:
        condition: service_started
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: catalogowebapi
    networks:
      catalogo: null
      web: null
    ports:
    - mode: ingress
      target: 80
      published: "5001"
      protocol: tcp
    - mode: ingress
      target: 443
      published: "5000"
      protocol: tcp
    - mode: ingress
      target: 5000
      published: "8080"
      protocol: tcp
    volumes:
    - type: bind
      source: C:\Users\kevin\AppData\Roaming/Microsoft/UserSecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\kevin\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
  db:
    environment:
      MYSQL_DATABASE: catalogo
      MYSQL_PASSWORD: dbuserpassword
      MYSQL_ROOT_PASSWORD: root
      MYSQL_USER: dbuser
    image: mysql:5.7
    networks:
      catalogo: null
    ports:
    - mode: ingress
      target: 3306
      published: "42333"
      protocol: tcp
    restart: always
    volumes:
    - type: volume
      source: dbdata
      target: /var/lib/mysql
      volume: {}
    - type: bind
      source: C:\Users\kevin\source\repos\atos\Catalogo\_MySQL_Init_Script
      target: /docker-entrypoint-initdb.d
      bind:
        create_host_path: true
networks:
  catalogo:
    name: dockercompose12082744716569367158_catalogo
  web:
    name: shared-web
volumes:
  dbdata:
    name: dockercompose12082744716569367158_dbdata